{
    "env": {
      "commonjs": false,
      "es2020": true,
      "node": true
    },
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["prettier"],
    "parserOptions": {
      "ecmaVersion": 11
    },
    "rules": {
      "prettier/prettier": "error",
      "no-underscore-dangle": "off",
      "no-console": "off",
      "camelcase": "off",
      "class-methods-use-this": "off",
      "prefer-destructuring": "off",
      "no-unused-vars": ["error", { "argsIgnorePattern": "next" }]
    }
  }
